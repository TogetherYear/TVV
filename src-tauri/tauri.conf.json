{
  "$schema": "https://schema.tauri.app/config/2.0.2",
  "productName": "Application",
  "version": "0.1.0",
  "identifier": "com.application.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:6768",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../Dist"
  },
  "app": {
    "windows": [
      {
        "width": 1000,
        "height": 560,
        "resizable": true,
        "center": true,
        "title": "Application",
        "decorations": false,
        "focus": false,
        "label": "Application",
        "transparent": true,
        "url": "/#/Application",
        "visible": false
    }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
